{
  "name": "fee-api",
  "version": "0.0.1",
  "description": "fee api server with node 8.0",
  "main": "app.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "start": "node app",
    "test": "jest --config ./__tests__/util/jest-config.json",
    "cross-env": "cross-env",
    "update-db": "knex migrate:latest --knexfile ./gen/knex/knexfile.js --cwd .",
    "update-test-db": "knex migrate:latest --knexfile ./gen/knex/knexfile.js --env test --cwd ."
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Andrew",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.5",
    "body-parser": "1.19.0",
    "express": "4.17.3",
    "express-request-id": "1.4.1",
    "jwt-simple": "0.5.6",
    "knex": "0.17.3",
    "log4js": "4.3.1",
    "log4js-json-layout": "^2.1.0",
    "mysql": "2.17.1",
    "objection": "1.6.9",
    "objection-db-errors": "1.1.1",
    "oracledb": "^3.1.2",
    "redis": "2.8.0",
    "run-sequence": "2.2.1"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "jest": "24.8.0",
    "gulp": "4.0.2",
    "gulp-help": "1.6.1",
    "gulp-template": "5.0.0",
    "yargs": "13.2.4"
  }
}
